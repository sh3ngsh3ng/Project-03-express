{{#extends 'base'}}

{{#block 'content'}}
{{#if error_messages}}
    <div class="alert alert-danger">
        {{error_messages}}
    </div>
{{/if}}



<h1>You can log in here!</h1>
<form method="POST">
    <input type="hidden" name="_csrf" value="{{csrfToken}}"/>
    {{{form}}}
    <input type="submit" class="btn btn-primary" value="Login" />
</form>

{{/block}}


{{/extends}}